{
  "name": "debugflow-complete",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "socket-server": "node server/index.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run socket-server\"",
    "dev:setup": "npm rebuild sqlite3 --build-from-source && npm run prepare:db",
    "prepare:db": "node -e \"import('./database/database.js').then(m => m.default.initialize()).then(() => console.log('Database ready')).catch(console.error)\"",
    "setup": "bash .devcontainer/setup.sh",
    "rebuild:sqlite": "npm rebuild sqlite3 --build-from-source",
    "clean": "rm -rf node_modules package-lock.json dist",
    "clean:install": "npm run clean && npm install",
    "postinstall": "npm rebuild sqlite3 2>/dev/null || true"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@google/generative-ai": "^0.24.1",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.4.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.1",
    "groq-sdk": "^0.30.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.259.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "openai": "^5.12.0",
    "postcss": "^8.4.0",
    "react": "^18.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.0.0",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "^2.4.0",
    "react-router-dom": "^7.8.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.5.0",
    "sqlite3": "^5.1.6",
    "tailwindcss": "^3.4.0",
    "terser": "^5.43.1",
    "typescript": "^5.9.2",
    "uuid": "^9.0.0",
    "vite": "^5.4.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.0.2",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "vercel": "^46.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
