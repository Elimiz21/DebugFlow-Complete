# Production Environment Configuration
# These values should be set in Vercel environment variables

# API Configuration
VITE_API_URL=/api
VITE_SOCKET_URL=wss://debug-flow-complete-7lnj.vercel.app

# Security
JWT_SECRET=your-production-jwt-secret-here
ADMIN_PASSWORD=your-secure-admin-password
ADMIN_SECRET=your-admin-jwt-secret

# Database
DATABASE_URL=your-production-database-url

# External APIs (Configure in Admin Panel)
OPENAI_API_KEY=
GROQ_API_KEY=
GEMINI_API_KEY=
ANTHROPIC_API_KEY=
GITHUB_TOKEN=

# Email Service
EMAIL_PROVIDER=sendgrid
SENDGRID_API_KEY=

# Storage
STORAGE_PROVIDER=s3
AWS_S3_BUCKET=
AWS_S3_REGION=
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# Monitoring
SENTRY_DSN=
LOGROCKET_APP_ID=

# Feature Flags
ENABLE_2FA=false
ENABLE_WEBHOOKS=false
ENABLE_RATE_LIMITING=true
ENABLE_CACHING=true

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Cache Configuration
REDIS_URL=
CACHE_TTL=3600